sp {op_find*propose*ask-for-help
   (state <s> ^name op_find1
              ^target-object <obj>
             -^current-action.stop-asking true)
-->
   (<s> ^operator <o> + =)
   (<o> ^name ask-for-help)
}

sp {op_find*apply*ask-for-help*send*message
   (state <s> ^name op_find1
              ^operator.name ask-for-help
              ^target-object <obj>
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure <f>)
   (<f> ^type cant-find-object
        ^object <obj>)
}
        
