sp {op_find1*prefer*think*recall*over*scan
   (state <s> ^name op_find1
              ^operator <o1> +
              ^operator <o2> +)
  (<o1> ^name << op_think1 op_recall1 >>)
   (<o2> ^name op_scan1)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_find1*prefer*scan*over*explore
   (state <s> ^name op_find1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name op_scan1
         ^category exploration)
   (<o2> ^name op_explore1
         ^category exploration)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_find1*prefer*learn-task-subaction*over*others
   (state <s> ^name op_find1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name learn-task-subaction)
   (<o2> ^category << stm ltm interaction exploration >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_find1*equal*preference*same*category
   (state <s> ^name op_find1
              ^operator <o1> +
              ^operator <o2> +)
  (<o1> ^category <cat>)
   (<o2> ^category <cat>)
-->
   (<s> ^operator <o1> = <o2>)
}

sp {op_find1*prefer*stm*over*others
   (state <s> ^name op_find1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^category stm)
   (<o2> ^category << ltm interaction exploration >>)
-->
