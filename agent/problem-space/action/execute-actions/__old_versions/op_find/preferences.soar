sp {op_find1*reject*subaction*if*not*matched
   (state <s> ^name op_find1
              ^operator <o> +
              ^action-matched false)
   (<o> ^category <any>)
-->
   (<s> ^operator <o> -)
}

sp {op_find1*reject*ask-for-help*if*child-action
   (state <s> ^name op_find1
              ^operator <o> +
              ^child-action <a>)
   (<o> ^name ask-for-help)
-->
   (<s> ^operator <o> -)
}

sp {op_find1*reject*report-success*if*child-action
   (state <s> ^name op_find1
              ^operator <o> +
