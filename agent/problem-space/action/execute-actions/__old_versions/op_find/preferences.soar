sp {op_find1*categorize*ltm
   (state <s> ^name op_find1
              ^operator <o> +
             -^child-action.execution-operator <o>)
   (<o> ^name << op_go-to-location1 op_face1 >> )
-->
   (<o> ^category stm)
}

sp {op_find1*categorize*ltm
   (state <s> ^name op_find1
              ^operator <o> +
             -^child-action.execution-operator <o>)
   (<o> ^name << op_scan1 op_recall1 op_think1 >> )
-->
   (<o> ^category ltm)
}

