sp {op_find1*reject*op_scan*if*scanned
   (state <s> ^name op_find1
              ^operator <o> +
              ^world.robot.current-location.handle <handle>
              ^current-action.scanned <handle>
              -^child-action.execution-operator <o>)
   (<o> ^name op_scan1)
-->
   (<s> ^operator <o> -)
}

sp {op_find1*apply*op_scan*mark*scanned
   (state <s> ^name op_find1
              ^operator.name op_scan1
              ^child-action.status << success failure >>
              ^world.robot.current-location.handle <handle>
              ^current-action <a>)
-->
   (<a> ^scanned <handle>)
}

sp {op_find1*apply*op_explore*mark*explored
   (state <s> ^name op_find1
              ^operator.name op_explore1
              ^child-action.status << success failure >>
              ^current-action <a>)
-->
   (<a> ^explored true)
}

sp {op_find1*reject*op_explore*if*explored
   (state <s> ^name op_find1
              ^operator <o> +
              ^current-action.explored true
              -^child-action.execution-operator <o>)
   (<o> ^name op_explore1)
-->
   (<s> ^operator <o> -)
}
