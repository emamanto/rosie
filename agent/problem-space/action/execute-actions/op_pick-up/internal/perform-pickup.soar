sp {op_pick-up1*internal*propose*perform-pickup
   (state <s> ^name op_pick-up1
              ^problem-space.action.type execute
              ^target-object.predicates.arm-status not-grabbed)
-->
   (<s> ^operator <o> +)
   (<o> ^name perform-pickup)
}

sp {op_pick-up1*internal*apply*perform-pickup*arm-status
   (state <s> ^name op_pick-up1
              ^problem-space.action.type execute
              ^operator.name perform-pickup
              ^target-object.predicates <preds>)
   (<preds> ^arm-status not-grabbed)
-->
   (<preds> ^arm-status not-grabbed -
            ^arm-status grabbed)
}

sp {op_pick-up1*internal*apply*perform-pickup*remove*predicates
   (state <s> ^name op_pick-up1
              ^problem-space.action.type execute
              ^operator.name perform-pickup
              ^world.predicates.predicate <pred>
              ^target-object <obj>)
   (<pred> ^instance <i>)
   (<i> ^{ << 1 2 3 4 5 >> } <obj>)
-->
   (<pred> ^instance <i> -)
}