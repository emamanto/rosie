sp {op_push1*fetch*elaborate*problem-space*attend-to-all-perception
   (state <s> ^name op_push1
              ^problem-space.action.type execute
              ^problem-space.perception <p>)
-->
   (<p> ^attend-to-all-perception yes)
}

sp {op_push1*fetch*elaborate*target-object*superoperator
   (state <s> ^name op_push1
              ^problem-space.action.type execute
              ^action-operator.arg1.id <obj>)
-->
   (<s> ^target-object <obj>)
}

sp {op_push1*fetch*elaborate*predicate-handle*superoperator
   (state <s> ^name op_push1
              ^problem-space.action.type execute
              ^action-operator.arg2.handle <pred-handle>)
-->
	(<s> ^predicate-handle <pred-handle>)
}

sp {op_push1*fetch*elaborate*destination*superoperator
   (state <s> ^name op_push1
              ^problem-space.action.type execute
              ^action-operator.arg2.{2} <obj>)
-->
   (<s> ^destination <obj>)
}

sp {op_push1*fetch*elaborate*projection-result*from*current-action
   (state <s> ^name op_push1
              ^problem-space.action.type execute
              ^current-action.projection-result <res>)
-->
   (<s> ^projection-result <res>)
}

#### GOAL SATISFACTION ####
sp {op_push1*fetch*elaborate*desired*satisfied
   (state <s> ^name op_push1
              ^problem-space.action.type execute
              ^current-action.push-command.status success)
-->
   (<s> ^desired.satisfied true)
}

sp {op_push1*fetch*elaborate*executing-command*true
   (state <s> ^name op_push1
              ^current-action.{ << push-command reset-command >> }.status << sent executing >>)
-->
   (<s> ^executing-command true)
}

sp {op_push1*fetch*elaborate*allow-wait*true
   (state <s> ^name op_push1
              ^executing-command true)
-->
   (<s> ^propose-wait true)
}
