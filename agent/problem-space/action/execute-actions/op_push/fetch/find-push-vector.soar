sp {op_push1*fetch*propose*find-push-vector
   (state <s> ^name op_push1
              ^problem-space.action.type execute
             -^current-action.projection-result
              ^target-object <target>
              ^destination <dest>
              ^predicate-handle <pred-handle>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name find-push-vector
        ^predicate-handle <pred-handle>
        ^arg-list <args>)
   (<args> ^1 <target> ^2 <dest>)
   (interrupt)
}

sp {op_push1*fetch*apply*find-push-vector*copy*result
   (state <s> ^name op_push1
              ^operator.name find-push-vector
              ^projection-result <res>
              ^current-action <a>)
-->
   (<a> ^projection-result <res>)
}
